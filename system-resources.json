{
  "checks":{
    "memory-usage":{
      "command":"check-memory-percent.rb -w 85 -c 95",
      "standalone":true,
      "occurrences":3,
      "publish":true,
      "interval":60,
      "ttl": 130
    },
    "disk-usage":{
      "command":"check-disk-usage.rb -w 70 -c 85 -K 85 -W 70",
      "standalone":true,
      "occurrences":2,
      "publish":true,
      "interval":600,
      "ttl": 900
    }
  }
}